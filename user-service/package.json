{
  "name": "user-service",
  "version": "1.0.0",
  "main": "/build/server.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate ./src/migrations/$npm_config_filename -t -d ./src/db/config.ts",
    "migration:up": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/db/config.ts",
    "migration:down": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/db/config.ts",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create ./src/migrations/$npm_config_filename",
    "dev": "nodemon"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^16.18.126",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "typescript-transform-paths": "^3.5.5"
  }
}
